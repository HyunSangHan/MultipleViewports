{
  "name": "browsersync",
  "version": "1.0.5",
  "description": "View desktop and mobile UI at the same time!",
  "scripts": {
    "build": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "check-types": "tsc",
    "lint": "tslint --project tsconfig.json",
    "compile": "babel src -d lib --extensions '.ts,.tsx'",
    "test": "mocha",
    "prepare": "npm run check-types && npm run lint && npm test && npm run compile && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HyunSangHan/WhaleBrowserSync.git"
  },
  "keywords": [
    "whale",
    "extension",
    "browser",
    "sync"
  ],
  "author": "HyunSangHan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HyunSangHan/WhaleBrowserSync/issues"
  },
  "homepage": "https://github.com/HyunSangHan/WhaleBrowserSync#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/chrome": "0.0.91",
    "@types/mocha": "^7.0.2",
    "@types/naver-whale": "0.0.0",
    "@types/node": "^13.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "file-loader": "^5.0.2",
    "fs-extra": "^8.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "mocha": "^10.2.0",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  },
  "lint-staged": {
    "*.ts": "tslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
